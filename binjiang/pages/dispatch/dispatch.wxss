/* pages/dispatch/dispatch.wxss */
.li{ width: 640rpx; border: 1px solid #d2d9e9; font-size: 16px; color: #fff; margin: 20rpx auto;padding: 20rpx; border-radius: 6rpx; background: rgba(0, 0, 0, 0.3); padding-left: 60rpx; line-height: 66rpx; position: relative; box-sizing: border-box;} 
.ul{width:750rpx;height: 1050rpx; white-space: nowrap; display: inline-block; }
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.detiletext{  width: 360rpx;white-space:pre-line;   display: inline-block; vertical-align: top;}
.color{ width: 50rpx; height: 50rpx; font-size: 14px; border-radius: 50%; background: blueviolet; line-height: 50rpx; text-align: center; position: absolute; left: -7rpx; top: -7rpx;}
.viewbottom{ width: 100%; display: inline-block; text-align: center;position: absolute; bottom: 20rpx; left: 0rpx;}
.viewmargin{  display:inline-block; border-radius: 10rpx; overflow: hidden}
.viewview{ background:#156ec0; width: 320rpx; color: #fff; display: inline-block; line-height: 80rpx; text-align: center;  }
.naviga{width: 640rpx; height: 100%; display: block}
.blue_hover{ background: rgba(21, 110, 192, 0.8);}
.active{ background: #11a0f8;}
.mainbg{ width: 1500rpx; height: 1050rpx; display: inline-block;}
.tabbg{ width: 750rpx; height: 1050rpx; }

